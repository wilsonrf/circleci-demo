files:
    "/root/.ssh/config":
        owner: root
        group: root
        mode: "000600"
        content: |
            Host github.com
                User git
                Hostname github.com
                IdentityFile /root/.ssh/github-deploy
    "/root/.ssh/known_hosts":
        owner: root
        group: root
        mode: "000644"
        content: |
            |1|O+CVEjdp3rUJqJXmYF25CMx9ylg=|7dBHqJK9/iqxBLIQgwGgPt11ldY= ssh-rsa 
commands:
    01-command:
        command: sudo aws s3 cp s3://gradient-deployment-key/github-deploy /root/.ssh
    02-command:
        command: sudo chmod 600 /root/.ssh/github-deploy